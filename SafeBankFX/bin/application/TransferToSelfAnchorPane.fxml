                        <Insets bottom="10.0" left="100.0" right="100.0" top="10.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="btnTransfer" mnemonicParsing="false" onAction="#handleTransfer" text="Transfer">
